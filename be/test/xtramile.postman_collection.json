	"item": [
		{
			"name": "create patient",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"function randomString(minValue, maxValue, dataSet = 'abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ') {",
							"    if (!minValue) {",
							"        minValue = 20;",
							"        maxValue = 20;",
							"    }",
							"",
							"    if (!maxValue) {",
							"        maxValue = minValue;",
							"    }",
							"",
							"    let length = _.random(minValue, maxValue),",
							"        randomString = \"\";",
							"",
							"    for (let i = 0; i < length; i++)",
							"        randomString += dataSet.charAt(Math.floor(Math.random() * dataSet.length));",
							"    return randomString;",
							"}",
							"",
							"pm.variables.set('fname', randomString(10,10));",
							"pm.variables.set('lname', randomString(10,10));",
							"pm.variables.set('phone', randomString(15,15, '1234567890'));"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"auth": {
					"type": "noauth"
				},
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"firstName\": \"{{fname}}\",\n    \"lastName\": \"{{lname}}\",    \n    \"dateOfBirth\": \"1995-12-01\",\n    \"gender\": \"M\",\n    \"phone\": \"{{phone}}\",\n    \"address\": \"john stree\",\n    \"suburb\": \"somewhere only we know\",\n    \"state\": \"victoria\",\n    \"postcode\": \"9999\"    \n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "http://localhost:8080/v1/patients",
					"protocol": "http",
